makeCacheMatrix <- function (x = matrix ()) {
invrs <- NULL
set <- function (y) {
x <- y
invrs <- NULL
}

get <- function () x
setinverse <- function (inverse) invrs <- inverse
getinverse <- function ()
lista de invrs (set = set, get = get,
setinverse = setinverse,
getinverse = getinverse)
}

cacheSolve <- function (x, ...) {
invrs <= x $ getinverse ()
if (! is.null (invrs)) {
message ("obteniendo datos en caché")
return (invrs)
}

mat <- x $ get ()
invrs <- solve (mat, ...)
x $ setinverse (invrs)
invrs
}
